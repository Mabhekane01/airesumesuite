{
  "name": "document-sharing-service",
  "version": "1.0.0",
  "description": "Enterprise-grade document sharing and analytics platform",
  "main": "packages/api-gateway/src/index.ts",
  "scripts": {
    "dev": "concurrently \"pnpm run dev:api\" \"pnpm run dev:analytics\" \"pnpm run dev:file-processor\" \"pnpm run dev:notifications\"",
    "dev:api": "cd packages/api-gateway && pnpm run dev",
    "dev:analytics": "cd packages/analytics-engine && pnpm run dev",
    "dev:file-processor": "cd packages/file-processor && pnpm run dev",
    "dev:notifications": "cd packages/notification-service && pnpm run dev",
    "build": "pnpm run build:api && pnpm run build:analytics && pnpm run build:file-processor && pnpm run build:notifications",
    "build:api": "cd packages/api-gateway && pnpm run build",
    "build:analytics": "cd packages/analytics-engine && pnpm run build",
    "build:file-processor": "cd packages/file-processor && pnpm run build",
    "build:notifications": "cd packages/notification-service && pnpm run build",
    "start": "cd packages/api-gateway && pnpm start",
    "test": "pnpm run test:api && pnpm run test:analytics && pnpm run test:file-processor && pnpm run test:notifications",
    "test:api": "cd packages/api-gateway && pnpm test",
    "test:analytics": "cd packages/analytics-engine && pnpm test",
    "test:file-processor": "cd packages/file-processor && pnpm test",
    "test:notifications": "cd packages/notification-service && pnpm test",
    "lint": "pnpm run lint:api && pnpm run lint:analytics && pnpm run lint:file-processor && pnpm run lint:notifications",
    "lint:api": "cd packages/api-gateway && pnpm run lint",
    "lint:analytics": "cd packages/analytics-engine && pnpm run lint",
    "lint:file-processor": "cd packages/file-processor && pnpm run lint",
    "lint:notifications": "cd packages/notification-service && pnpm run lint",
    "docker:build": "docker-compose build",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "db:migrate": "cd packages/core && pnpm run migrate",
    "db:seed": "cd packages/core && pnpm run seed",
    "clean": "rm -rf node_modules packages/*/node_modules packages/*/dist",
    "type-check": "pnpm run type-check:api && pnpm run type-check:analytics && pnpm run type-check:file-processor && pnpm run type-check:notifications",
    "type-check:api": "cd packages/api-gateway && pnpm run type-check",
    "type-check:analytics": "cd packages/analytics-engine && pnpm run type-check",
    "type-check:file-processor": "cd packages/file-processor && pnpm run type-check",
    "type-check:notifications": "cd packages/notification-service && pnpm run type-check"
  },
  "keywords": [
    "document-sharing",
    "analytics",
    "file-processing",
    "enterprise",
    "api",
    "typescript",
    "nodejs"
  ],
  "author": "AI Job Suite Team",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "concurrently": "^8.0.0",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "prettier": "^3.0.0",
    "typescript": "^5.0.0"
  },
  "workspaces": [
    "packages/*"
  ],
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=9.0.0"
  },
  "packageManager": "pnpm@9.12.3"
}

{
  "name": "@ai-job-suite/auth-service",
  "version": "1.0.0",
  "description": "Centralized authentication service for AI Job Suite ecosystem",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "packageManager": "pnpm@9.12.3",
  "scripts": {
    "build": "tsc",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "start": "node dist/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "clean": "rm -rf dist",
    "db:migrate": "ts-node src/database/migrate.ts",
    "db:seed": "ts-node src/database/seed.ts",
    "db:reset": "ts-node src/database/reset.ts",
    "docker:build": "docker build -t ai-job-suite-auth .",
    "docker:run": "docker run -p 3001:3001 ai-job-suite-auth",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "docker:clean": "docker-compose down -v --remove-orphans"
  },
  "dependencies": {
    "express": "^4.21.1",
    "cors": "^2.8.5",
    "helmet": "^8.1.0",
    "express-rate-limit": "^7.4.1",
    "compression": "^1.7.5",
    "dotenv": "^16.4.5",
    "bcryptjs": "^3.0.2",
    "jsonwebtoken": "^9.0.2",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pg": "^8.13.1",
    "redis": "^5.8.2",
    "uuid": "^11.0.3",
    "winston": "^3.17.0",
    "express-validator": "^7.2.0",
    "nodemailer": "^6.9.16",
    "node-cron": "^3.0.3",
    "express-session": "^1.18.1",
    "connect-redis": "^7.1.1",
    "multer": "^1.4.5-lts.1",
    "geoip-lite": "^1.4.10",
    "ua-parser-js": "^2.0.0"
  },
  "devDependencies": {
    "@types/express": "^5.0.0",
    "@types/cors": "^2.8.19",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/passport": "^1.0.16",
    "@types/passport-google-oauth20": "^2.0.16",
    "@types/passport-jwt": "^4.0.1",
    "@types/passport-local": "^1.0.38",
    "@types/pg": "^8.11.10",
    "@types/uuid": "^10.0.0",
    "@types/nodemailer": "^6.4.19",
    "@types/multer": "^1.4.12",
    "@types/express-session": "^1.18.0",
    "@types/node": "^22.8.6",
    "@types/node-cron": "^3.0.11",
    "@types/compression": "^1.7.5",
    "typescript": "^5.6.3",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.14",
    "ts-jest": "^29.2.5",
    "eslint": "^9.14.0",
    "@typescript-eslint/eslint-plugin": "^8.12.2",
    "@typescript-eslint/parser": "^8.12.2"
  },
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=9.0.0"
  },
  "keywords": [
    "authentication",
    "auth",
    "jwt",
    "oauth",
    "postgresql",
    "redis",
    "microservice"
  ],
  "author": "AI Job Suite Team",
  "license": "MIT"
}

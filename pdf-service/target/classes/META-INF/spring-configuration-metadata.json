    {
      "name": "rate.limit.requests.per.minute", 
      "type": "java.lang.Integer",
      "description": "Maximum requests per minute per client",
      "defaultValue": 60
    },
    {
      "name": "rate.limit.requests.per.hour",
      "type": "java.lang.Integer",
      "description": "Maximum requests per hour per client", 
      "defaultValue": 1000
    },
    {
      "name": "ocr.service.url",
      "type": "java.lang.String",
      "description": "External OCR service URL"
    },
    {
      "name": "ocr.service.api-key",
      "type": "java.lang.String",
      "description": "External OCR service API key"
    },
    {
      "name": "ocr.service.timeout",
      "type": "java.lang.Integer", 
      "description": "OCR service timeout in milliseconds",
      "defaultValue": 30000
    }
